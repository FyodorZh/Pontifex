<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>9</LangVersion>
        <Nullable>disable</Nullable>
        <RootNamespace>UnityUtils.Memory</RootNamespace>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Logger\Log\Log.csproj" />
      <ProjectReference Include="..\Serializer\Serializer.csproj" />
      <ProjectReference Include="..\UnityUtils.LifeTime\UnityUtils.LifeTime.csproj" />
      <ProjectReference Include="..\UnityUtils\UnityUtils.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Archivarius.Tools" Version="0.2.1-dev.4" />
    </ItemGroup>

</Project>
